<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="corner" format="reference|dimension" />

    <declare-styleable name="CornerTextView">
        <attr name="corner" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="ShapeTextView">
        <attr name="corner" />
        <attr name="solid" format="color" />
    </declare-styleable>

    <declare-styleable name="SideBar">
        <attr name="android:textSize" format="reference|dimension" />
        <attr name="android:textColor" format="color" />
    </declare-styleable>
</resources>